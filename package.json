{
	"name": "tauri-vscode",
	"displayName": "Tauri",
	"version": "0.0.1",
	"private": false,
	"description": "Enhances the experience of Tauri apps development",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/TauriLand#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/TauriLand/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/TauriLand.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "Tauri",
				"command": "tauri.init",
				"title": "Init"
			},
			{
				"category": "Tauri",
				"command": "tauri.dev",
				"title": "Dev"
			},
			{
				"category": "Tauri",
				"command": "tauri.build",
				"title": "Build"
			},
			{
				"category": "Tauri",
				"command": "tauri.build-debug",
				"title": "Build with debug"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": [
					"tauri.conf.json",
					"tauri.conf.json5",
					"tauri.*.conf.json",
					"tauri.*.conf.json5"
				],
				"url": "tauri://schemas/config.json"
			}
		],
		"snippets": [
			{
				"language": "rust",
				"path": "./snippets/rust.json"
			}
		]
	},
	"activationEvents": [
		"workspaceContains:**/tauri.conf.json",
		"workspaceContains:**/tauri.*.conf.json",
		"workspaceContains:**/tauri.conf.json5",
		"workspaceContains:**/tauri.*.conf.json5",
		"onFileSystem:tauri"
	],
	"dependencies": {
		"axios": "1.6.8",
		"glob": "10.3.12",
		"run-in-terminal": "0.0.3",
		"strip-ansi": "6.0.1"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "18.19.31"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "assets/icon.png"
}
